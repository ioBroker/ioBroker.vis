{
    "version": "2.10.8",
    "packages": ["packages/*"],
    "command": {
        "run": {
            "stream": true
        },
        "version": {
            "npmClientArgs": ["-f"],
            "allowBranch": ["master"],
            "ignoreChanges": ["*.md"],
            "exact": true,
            "forcePublish": true
        }
    },
    "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}
